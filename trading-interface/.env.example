NEXT_PUBLIC_APTOS_NODE_URL=aptos-node-url
NEXT_PUBLIC_COINMARKETCAP_API_KEY=cmc-api-key
NEXT_PUBLIC_COINGECKO_API_KEY=cg-api-key

# WalletConnect project ID
# https://cloud.reown.com/
NEXT_PUBLIC_PROJECT_ID=project-id

# The URL of your Supabase instance
SUPABASE_URL=supabase-url

# Service role key with admin privileges for server-side operations
# All database actions will be performed from the server
SUPABASE_SERVICE_ROLE_KEY=supabase-service-role-key

# OpenAI API key for the AI chat functionality
OPENAI_API_KEY=openai-api-key

# JWT secret key for session management - use a strong random string (min 32 chars)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET_KEY=run-the-command-to-get-secret-key

# deBridge API endpoints
DLN_STATS_API_URL=https://stats-api.dln.trade/api
DLN_API_URL=https://api.dln.trade/v1.0/dln
DLN_EXPLORER_URL=https://app.debridge.finance

# CoinGecko API URL
COINGECKO_API_URL=https://api.coingecko.com/api/v3

# Odos API URL
ODOS_API_URL=https://api.odos.xyz

# Contract address for the AI Trading Assistant
NEXT_PUBLIC_CONTRACT_ADDRESS=your-contract-address


# Private key for the assistant wallet (stored securely)
ASSISTANT_PRIVATE_KEY=assistant-pk
EXECUTOR_PRIVATE_KEY=executor-pk

# Market data API endpoint for cryptocurrency price and market information
MARKET_DATA_API_URL=https://pro-api.coinmarketcap.com

# API key for accessing the market data service
MARKET_DATA_API_KEY=market-data-api-key
